              "options": [
                {
                  "descType": "text",
                  "description": "<p><math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mfrac><mi>r</mi><msqrt><mn>2</mn></msqrt></mfrac></math></p>",
                  "isCorrect": true,
                  "_id": "664c8f9ad950a07ad6320c60"
                },
                {
                  "descType": "text",
                  "description": "<p><math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mfrac><mrow><mi mathvariant=\"normal\">π</mi><mi>r</mi></mrow><msqrt><mn>2</mn></msqrt></mfrac></math></p>",
                  "isCorrect": false,
