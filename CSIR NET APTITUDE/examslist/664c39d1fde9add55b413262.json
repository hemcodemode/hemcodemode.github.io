                  "_id": "66419fbac050bbd24c3a961c"
                }
              ],
              "options": [
                {
                  "descType": "text",
                  "description": "<p>24</p>",
                  "isCorrect": false,
                  "_id": "66419fbac050bbd24c3a961d"
                },
                {
                  "descType": "text",
                  "description": "<p>30</p>",
                  "isCorrect": false,
                  "_id": "66419fbac050bbd24c3a961e"
                },
                {
                  "descType": "text",
                  "description": "<p>33</p>",
                  "isCorrect": true,
                  "_id": "66419fbac050bbd24c3a961f"
